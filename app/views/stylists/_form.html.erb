<%= form_for @stylist do |f| %>
    <%= f.label :name %>
        <%= f.text_field :name %>
    <%= f.label :phone_number %>
        <%= f.phone_field :phone_number, placeholder: "xxxxxxxxxx"  %>
    <%= f.label :yrs_experience %>
        <%= f.number_field :yrs_experience %>
    <%= f.submit %>
<% end %>

    