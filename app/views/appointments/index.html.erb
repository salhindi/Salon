<%= month_calendar(events: @appointments) do |date, appointments| %>
  <%= date %>

  <% appointments.each do |appointment| %>
    <div>
Client: <%= appointment.client.name %>
      <br> Stylist: <%= appointment.stylist.name.capitalize %>
      <br>
      <%= appointment.day.strftime("%H:%M") %>



    </div>
  <% end %>
<% end %>